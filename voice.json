    "voice_msec": 999
  },
  {
    "id": 100021,
    "sound_id": 530001,
    "on_flag": 1,
    "text": "いらっしゃいませ！　いろいろ揃えてるよ。",
    "voice_msec": 2870
  },
  {
    "id": 100022,
    "sound_id": 530002,
    "on_flag": 1,
    "text": "ああ～今日はヒマだ…。",
    "voice_msec": 2969
  },
  {
    "id": 100023,
    "sound_id": 530003,
    "on_flag": 1,
    "text": "冒険？　私も連れて行ってよ。",
    "voice_msec": 2691
  },
  {
    "id": 100024,
    "sound_id": 530004,
    "on_flag": 1,
    "text": "冒険に大切なのは事前の準備だよ。",
    "voice_msec": 3307
  },
  {
    "id": 100025,
    "sound_id": 530005,
    "on_flag": 1,
    "text": "私はヴァージニア。みんな、ジニーって呼ぶわ。",
    "voice_msec": 3506
  },
  {
    "id": 100026,
    "sound_id": 530006,
    "on_flag": 0,
    "text": "ショップボイス6(非再生テスト用)",
    "voice_msec": 999
  },
  {
    "id": 100027,
