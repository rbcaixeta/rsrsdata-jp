    "voice_msec": 999
  },
  {
    "id": 100011,
    "sound_id": 520001,
    "on_flag": 1,
    "text": "鍛錬無くして進化は無いぞ。",
    "voice_msec": 2248
  },
  {
    "id": 100012,
    "sound_id": 520002,
    "on_flag": 1,
    "text": "ここは戦士を鍛えるための道場だ。",
    "voice_msec": 2716
  },
  {
    "id": 100013,
    "sound_id": 520003,
    "on_flag": 1,
    "text": "戦士の強化に努めよ。",
    "voice_msec": 1918
  },
  {
    "id": 100014,
    "sound_id": 520004,
    "on_flag": 1,
    "text": "侯国塔士団の道場にようこそ。",
    "voice_msec": 2653
  },
  {
    "id": 100015,
    "sound_id": 520005,
    "on_flag": 1,
    "text": "よく来た…さあ、始めよう。",
    "voice_msec": 2703
  },
  {
    "id": 100016,
    "sound_id": 520006,
    "on_flag": 0,
